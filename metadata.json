{
  "name": "andschwa-ghost",
  "version": "0.3.1",
  "author": "Andrew Schwartzmeyer",
  "summary": "Install and Manage Ghost Blogs",
  "license": "AGPL-3.0",
  "source": "git://github.com/andschwa/puppet-ghost.git",
  "project_page": "https://github.com/andschwa/puppet-ghost",
  "issues_url": "https://github.com/andschwa/puppet-ghost/issues",
  "requirements": [
    {
      "name": "pe",
      "version_requirement": "3.x"
    },
    {
      "name": "puppet",
