  "dependencies": [
    { "name": "puppetlabs/stdlib", "version_requirement": ">= 4.1.0 < 5.0.0" },
    { "name": "puppet/nodejs", "version_requirement": ">= 1.3.0 < 2.0.0" },
    { "name": "proletaryo/supervisor", "version_requirement": ">= 0.4.0 < 0.5.0" }
  ]
